parents = {}
for i in range(int(input())):
    a = input().split()
    parents[a[0]] = [] if len(a) == 1 else a[2:]


def is_parent(child, parent):
    if child == parent:
        return True

    for p in parents[child]:
        if is_parent(p, parent):
            return True
    return False


for i in range(int(input())):
    a, b = input().split()
    print("Yes" if is_parent(b, a) else "No")
